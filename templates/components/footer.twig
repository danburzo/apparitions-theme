<footer class="footer">
  <div class='grid'>
    <div class='footer__inner grid__column grid__column--span-3'>
      <nav class='navbar navbar--inline'>
        {% 
          include 'components/menu.twig' with {
            'menu': footer_menu.get_items
          } 
        %}
      </nav>
    </div>
    <div class='grid__column'><!-- dummy --></div>
  </div>
</footer>

{# Scripts for shortcodes #}
{% if post %}
  {% if post | has_shortcode('readmore') %}
    <script src='{{site.theme.link}}/static/js/shortcode-readmore.js'></script>
  {% endif %}
{% endif %}

